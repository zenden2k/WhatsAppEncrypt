{
    "name": "zenden2k/whatsapp-encrypt",
    "description": "WhatsApp encryption decorators for PSR-7 streams",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Sergey Svistunov",
            "email": "zenden2k@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zenden2k\\WhatsAppEncrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zenden2k\\WhatsAppEncrypt\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^2.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "guzzlehttp/psr7": "^2.7"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "test-coverage": "./vendor/bin/phpunit --coverage-html coverage"
    }
}
